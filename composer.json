{
	"name": "able/io",
	"type": "library",
	"keywords": ["phpable", "filesystem", "input", "output"],
	"description": "phpABLE file-system library",
	"license": "MIT",
	"authors": [
		{
			"name": "Kassius Kress",
			"email": "hacpaka@kamora.vn"
		}
	],
	"require": {
		"php": ">=7.4.0",
		"ext-fileinfo": "*",
		"able/fixes": "dev-master",
		"able/prototypes": "dev-master",
		"able/helpers": "dev-master"
	},
	"require-dev": {
		"phpunit/phpunit": "^7"
	},
	"autoload": {
		"psr-4": {
			"Able\\IO\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Able\\IO\\Tests\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
